    "scripts": {
        "prettier": "prettier --write \"**/*.ts\" \"**/*.js\" \"**/*.json\" \"**/*.css\"",
        "prettify": "pretty-quick --staged",
        "dev:basic": "nodemon src/index.ts",
        "dev": "concurrently \"npm run db-proxy\" \"npm run dev:basic\"",
        "start-ts": "ts-node src/index.ts",
        "start": "node dist/index.js",
        "build": "tsc",
        "generate-db-types": "kysely-codegen --out-file ./src/services/database/db.d.ts",
        "migrate-up": "ts-node scripts/run-migrations.ts",
        "migrate-down": "ts-node scripts/migrate-down.ts",
        "db-proxy": "fly proxy 5432 -a personal-assistant-db --bind-addr 127.0.0.1",
        "storage-ui": "flyctl storage dashboard personal-assistant-storage",
        "test-file-service": "ts-node scripts/test-file-service.ts",
        "test-gmail": "ts-node src/scripts/test-gmail-service.ts",
        "test-langchain": "jest src/__tests__/services/langchain.test.ts",
        "test-crypto": "jest src/__tests__/services/crypto.test.ts",
        "test-coinmarketcap": "ts-node src/scripts/test-coinmarketcap-service.ts",
        "token-manager": "ts-node src/scripts/tokenManager.ts",
        "crypto-portfolio": "ts-node src/scripts/cryptoPortfolio.ts",
        "gmail": "ts-node src/scripts/gmail.ts",
        "test-chart": "ts-node src/scripts/testChartGeneration.ts",
        "process-raiffeisen-emails": "ts-node src/commands/processRaiffeisenEmails.ts",
        "process-xtb-emails": "ts-node src/commands/processXtbEmails.ts",
        "test-xtb-processor": "ts-node src/scripts/test-xtb-email-processor.ts",
        "test-portfolio-summary": "ts-node src/scripts/testPortfolioSummary.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn prettify"
        }
    },
    "keywords": [
        "node",
        "ts",
        "typescript"
